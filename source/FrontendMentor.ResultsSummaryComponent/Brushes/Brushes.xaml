<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
                    xmlns:resourceDictionaries="clr-namespace:FrontendMentor.Assets.ResourceDictionaries;assembly=FrontendMentor.Assets">

    <ResourceDictionary.MergedDictionaries>
        <resourceDictionaries:SharedResourceDictionary Source="pack://application:,,,/FrontendMentor.ResultsSummaryComponent;component/Colors/Colors.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <!--#region ResultsSummaryComponentWindow-->

    <SolidColorBrush x:Key="ResultsSummaryComponentWindow.Background.Brush"
                     po:Freeze="True"
                     Color="{StaticResource VeryLightBlue.Color}" />

    <SolidColorBrush x:Key="ResultsSummaryComponentWindow.Foreground.Brush"
                     po:Freeze="True"
                     Color="{StaticResource DarkNavy.Color}" />

    <!--#endregion-->

    <!--#region ResultsSummaryComponentSectorView-->

    <SolidColorBrush x:Key="ResultsSummaryComponentSectorView.YourResultTextBlock.Foreground.Brush"
                     po:Freeze="True"
                     Color="{StaticResource LightBlue.Color}" />

    <!--#endregion-->

</ResourceDictionary>